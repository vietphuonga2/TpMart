{
  "name": "REFER_API",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "devlog": "tsc -p . && ts-node-dev dist/index.js",
    "dev": "tsoa spec-and-routes && concurrently \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "dev:win": "tsoa routes && concurrently  \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "dev:ignored": "nodemon --watch './**/*.ts'--delay 3  --exec 'ts-node' src/index.ts",
    "dev:tsoa": "tsoa routes && concurrently  \"nodemon --exec 'ts-node src/index.ts'\" \"nodemon -x tsoa spec\"",
    "dev:auto": "tsoa spec-and-routes && concurrently \"nodemon\" \"nodemon --watch './**/controllers/*.ts' -x tsoa spec-and-routes\"",
    "build:code": "tsc -p .",
    "build:route": "tsoa routes",
    "build:doc": "tsoa spec-and-routes",
    "build": " yarn build:doc && yarn build:code",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.169",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.17.0",
    "cors": "~2.8.1",
    "cross-env": "~3.2.3",
    "dotenv": "^6.1.0",
    "express": "~4.15.0",
    "express-subdomain": "^1.0.5",
    "express-validator": "^3.1.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "~1.8.1",
    "mysql": "^2.18.1",
    "mysql2": "^1.6.4",
    "node-html-parser": "^3.3.4",
    "node-schedule": "^2.0.0",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^6.2.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/express-validator": "^3.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mime-types": "^2.1.29",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsoa": "^3.7.0",
    "typescript": "^4.2.3",
    "typescript-formatter": "^7.2.2"
  },
  "_moduleAliases": {
    "@config": "./dist/config",
    "@controllers": "./dist/controllers",
    "@models": "./dist/models",
    "@routes": "./dist/routes",
    "@services": "./dist/services",
    "@utils": "./dist/utils",
    "@helpers": "./dist/helpers",
    "@commons": "./dist/commons",
    "@middleware": "./dist/middleware"
  }
}